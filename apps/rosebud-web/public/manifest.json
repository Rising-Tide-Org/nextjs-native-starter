{
    "name": "Rosebud",
    "short_name": "Rosebud",
    "description": "Rosebud is an AI-powered journal that helps you reflect on your day and manifest the life you want, in just 5 minutes a day.",
    "icons": [
      {
        "src": "/android-chrome-192x192.png",
        "sizes": "192x192",
        "type": "image/png",
        "purpose": "maskable"
      },
      {
        "src": "/android-chrome-512x512.png",
        "sizes": "512x512",
        "type": "image/png",
        "purpose": "any"
      }
    ],
    "shortcuts": [
      {
        "name": "New entry",
        "url": "/compose",
        "description": "Start a blank entry"
      },
      {
        "name": "View journal",
        "url": "/journal",
        "description": "View all your entries"
      }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "display": "standalone",
    "scope": "/",
    "start_url": "/signin",
    "orientation": "portrait",
    "screenshots": [],
    "protocol_handlers": [
      {
        "protocol": "web+rosebud",
        "url": "/?launch=%s"
      }
    ],
    "dir": "auto",
    "lang": "en",
    "prefer_related_applications": false,
    "categories": [
      "health",
      "lifestyle",
      "personalization",
      "productivity"
    ],
    "edge_side_panel": {
      "preferred_width": 400
    },
    "display_override": [
      "window-controls-overlay"
    ]
  }
